	<pluginRepository>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <id>plugins-nexus</id>
          <name>plugins-nexus</name>
	  <url>http://nexus.wdf.sap.corp:8081/nexus/content/repositories/deploy.snapshots/</url>
        </pluginRepository>
	 -->
</pluginRepositories>
<id>artifactory</id>
<activation>
<activeByDefault>true</activeByDefault>
</activation>
</profile>
</profiles>
<activeProfiles>
<!--  make the profile active all the time  -->
<activeProfile>artifactory</activeProfile>
</activeProfiles>
<!--  define the sonatype plugin group, so the nexus plugins will work without typing the groupId  -->
<pluginGroups>
<pluginGroup>org.sonatype.plugins</pluginGroup>
<pluginGroup>ariba.devtools</pluginGroup>
<pluginGroup>com.sap.ldi</pluginGroup>
<pluginGroup>org.whitesource</pluginGroup>
</pluginGroups>
<proxies>
<proxy>
<id>httpsproxy</id>
<active>true</active>
<protocol>https</protocol>
<host>proxy02.ariba.com</host>
<port>8080</port>
<nonProxyHosts>*.ariba.com</nonProxyHosts>
</proxy>
<proxy>
<id>myproxy</id>
<active>true</active>
<protocol>http</protocol>
<host>proxy02.ariba.com</host>
<port>8080</port>
<nonProxyHosts>*.ariba.com</nonProxyHosts>
</proxy>
</proxies>
</settings>
