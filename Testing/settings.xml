<servers>
<!--  this is the user name for deploying to nexus - developers
                              should not need this in their settings.
                                       -->
<server>
<id>deployment</id>
<username>deployment</username>
<password>ariba2og</password>
</server>
<server>
<id>deploy.releases</id>
<username>temp_admin</username>
<password>temp_admin</password>
</server>
<server>
<id>deploy.snapshots</id>
<username>temp_admin</username>
<password>temp_admin</password>
</server>
<server>
<id>deployment</id>
<username>deployment</username>
<password>deployment123</password>
</server>
</servers>
<profiles>
<profile>
<repositories>
<!--  for deploy testing - be sure to comment this out when finished testing  -->
