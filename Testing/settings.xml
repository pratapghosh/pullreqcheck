<repository>
<id>nexus</id>
<url>http://nexus1.ariba.com:8081/nexus/content/groups/build.snapshots/</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>true</enabled>
</snapshots>
</repository>
<repository>
<snapshots>
<enabled>false</enabled>
</snapshots>
<id>central</id>
<name>libs-release</name>
<url>http://registry.mo.sap.corp:80/libs-release</url>
</repository>
<repository>
<snapshots>
<enabled>true</enabled>
</snapshots>
<id>snapshots</id>
<name>libs-snapshot</name>
<url>http://registry.mo.sap.corp:80/libs-snapshot</url>
</repository>
<repository>
<id>repoaws</id>
<url>https://repo.aws.ariba.com/libs-release-local</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>false</enabled>
</snapshots>
</repository>
<repository>
<id>remote</id>
<url>http://registry.mo.sap.corp:80/remote-repos</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>true</enabled>
</snapshots>
</repository>
<repository>
<id>central-sap</id>
<url>http://nexus.wdf.sap.corp:8081/nexus/content/groups/build.milestones</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>true</enabled>
</snapshots>
</repository>
</repositories>
<pluginRepositories>
<pluginRepository>
<id>repoaws</id>
<url>https://repo.aws.ariba.com/libs-release-local</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>false</enabled>
</snapshots>
</pluginRepository>
<pluginRepository>
<snapshots>
<enabled>false</enabled>
</snapshots>
<id>central</id>
<name>plugins-release</name>
<url>http://registry.mo.sap.corp:80/plugins-release</url>
</pluginRepository>
<pluginRepository>
<snapshots/>
<id>snapshots</id>
<name>plugins-snapshot</name>
<url>http://registry.mo.sap.corp:80/plugins-snapshot</url>
</pluginRepository>
<pluginRepository>
<id>ariba-releases</id>
<url>http://nexus1.ariba.com:8081/nexus/content/groups/build.releases/</url>
<releases>
<enabled>true</enabled>
</releases>
<snapshots>
<enabled>true</enabled>
</snapshots>
</pluginRepository>
